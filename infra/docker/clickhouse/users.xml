<?xml version="1.0"?>
<clickhouse>
    <users>
        <!-- Default user (required by ClickHouse, used for healthchecks) -->
        <default>
            <password></password>
            <networks>
                <ip>::1</ip>
                <ip>127.0.0.1</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
        </default>

        <!-- MLRun application user -->
        <mlrun>
            <password>mlrun_dev</password>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </mlrun>
    </users>

    <profiles>
        <default>
            <!-- User-level settings for local dev -->
            <max_memory_usage>1073741824</max_memory_usage>
            <max_threads>4</max_threads>
            <max_insert_threads>2</max_insert_threads>
            <max_insert_block_size>1048576</max_insert_block_size>
            <max_partitions_per_insert_block>100</max_partitions_per_insert_block>
            <use_uncompressed_cache>0</use_uncompressed_cache>
            <load_balancing>random</load_balancing>
        </default>
    </profiles>

    <quotas>
        <default>
            <interval>
                <duration>3600</duration>
                <queries>0</queries>
                <errors>0</errors>
                <result_rows>0</result_rows>
                <read_rows>0</read_rows>
                <execution_time>0</execution_time>
            </interval>
        </default>
    </quotas>
</clickhouse>
